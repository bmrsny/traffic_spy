<h1>
  Url Stats!!!!
</h1>
<h2><%= url_shortest_response %></h2>
<h3>
  <strong><%= identifier.capitalize %></strong>
  <p>Where the path is:
    <%= url_relative_path %>
  </p>
</h3>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Shortest Response Time</th>
          <th>Url Path</th>
        </tr>
        <tbody>
          <tr>
            <td><%= url_shortest_response[0][1] %></td>
            <td><%= identifier %> <%= url_relative_path %></td>
          </tr>
        </tbody>
      </thead>
    </table>
  </div>
</div>
<h3><a href="/sources/<%= identifier %>"><<< Back to Identifier table</a></h3>
